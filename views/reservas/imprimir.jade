			height: 50px;
		}

		.barcode-line {
			height: inherit;
			display: inline-block;
			float: left;
		}
		@media print
		{
			* {-webkit-print-color-adjust:exact;}
			.no-print, .no-print *
			{
				display: none !important;
			}
		}

body
	div
		table.w666
	tr.cpN
	tr.ti
	td.At.Ac
		br
		form
		input.no-print(type='button', value='Clique aqui para imprimir', onclick='window.print();return false;')
	br
	br
	table.w666
		tr.BHead
			td(class='imgLogo')
				img(src='/img/home/logo.png')
	div.meiaConteudo
		h1 Reservas para #{reservas.modalidade}
		br
		p Nome: #{reservas.nomeUser}
		p Data: #{reservas.data}
		p Hora Início: #{reservas.horaInicio}
		p Hora Termíno: #{reservas.horaTermino}
		br
		h3 Participantes nome
		br
		//- p Número de Participantes: #{listaParticipantes.numeroParticipante}
		each lista, i in listaParticipantes
			br
			p Nome do Participante: #{lista.nomeParticipante}
			p RG do Participante: #{lista.rgParticipante}
